<div>
    <h5 class="font-semibold">Overdue</h5>
    <ul class="list-none">
  <% for(var i = 0; i < overdueItems.length; i++) { %>
    <li class="todo-item">
      <div
        class="flex items-center w-fit my-2 px-2 py-1 rounded hover:shadow-md hover:bg-slate-700"
      >
        <input
          id="todo-checkbox-o<%= i+1 %>"
          type="checkbox"
          class="w-4 h-4 text-blue-600 rounded"
        />
        <label class="text-gray-300 cursor-pointer ml-2" for="todo-checkbox-o<%= i+1 %>">
          <%- overdueItems[i].title %>
        </label>
        <%- include('deleteIcon.ejs') %>
      </div>
    </li>
    <% } %>
    </ul>
    <h5 class="font-semibold">Due Today</h5>
    <ul class="list-none">
  <% for(var i = 0; i < dueTodayItems.length; i++) { %>
